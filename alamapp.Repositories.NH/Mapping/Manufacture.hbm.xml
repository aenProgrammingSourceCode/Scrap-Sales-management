<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="alamapp.Model.Products"
        assembly="alamapp.Model">

  <class name="Manufacture" table="Manufactures" lazy="false" >
    <id  name="Id" column="ManufactureID"  type="int" unsaved-value="0">
      <generator class="native" />
    </id>

    <property name="Name">
      <column name="ManufactureName" sql-type="nvarchar" not-null="false" />
    </property>

  </class>
</hibernate-mapping> 
